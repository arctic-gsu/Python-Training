   "source": [
    "from scipy import optimize\n",
    "import numpy as np\n",
    "\n",
    "def func(x):\n",
    "    return [x[0] * np.cos(x[1]) - 4,\n",
    "            x[1] * x[0] - x[1] - 5]\n",
    "root = optimize.fsolve(func, [1, 1])\n",
    "print(root)\n",
    "\n",
    "np.isclose(func(root), [0.0, 0.0])  # func(root) should be almost 0.0.\n"
   ]
